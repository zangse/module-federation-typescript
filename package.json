{
  "name": "module-federation-typescript",
  "version": "2.4.2-patch.5",
  "description": "Webpack plugin to stream typescript for module federation apps/components",
  "license": "MIT",
  "type": "commonjs",
  "keywords": [
    "module-federation",
    "typescript",
    "remote types",
    "federated types"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "author": "Prasanna Mestha <prasanna@privjs.com> (https://github.com/prasannamestha)",
  "contributors": [
    "Zack Jackson <zackary.l.jackson@gmail.com> (https://github.com/ScriptedAlchemy)",
    "Pavan Divi <pavan.divi@outlook.com> (https://github.com/pavandv)"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch"
  },
  "peerDependencies": {
    "next": "*",
    "react": "*",
    "react-dom": "*",
    "typescript": "*",
    "vue-tsc": "^1.0.24",
    "webpack": "^5.75.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "vue-tsc": {
      "optional": true
    },
    "next": {
      "optional": true
    }
  },
  "main": "./src/index.js",
  "exports": {
    ".": {
      "require": "./src/index.js",
      "types": "./src/index.d.ts"
    }
  },
  "dependencies": {
    "axios": "^1.5.0",
    "fs": "^0.0.1-security",
    "lodash.get": "^4.4.2",
    "node-fetch": "2.6.4",
    "typescript": "4.7.4",
    "vue-tsc": "^1.0.24",
    "webpack": "^5.75.0"
  },
  "devDependencies": {
    "@types/lodash.get": "^4.4.7",
    "@types/node-fetch": "^2.6.4"
  }
}
